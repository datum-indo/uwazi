exports[`AttachmentsList should render a sorted list of attachments (files) 1`] = `
<div
  className="attachments-list-parent"
>
  <Connect(NeedAuthorization)>
    <div
      className="attachment-buttons main-file"
    >
      <h2>
        Document
        <Tip
          icon="question-circle"
        >
          Main file: add a file as the main content
        </Tip>
      </h2>
      <Connect(UploadButton)
        documentId="parentId"
        documentSharedId="parentSharedId"
        storeKey="storeKey"
      />
    </div>
  </Connect(NeedAuthorization)>
  <h2>
    Attachments
  </h2>
  <div
    className="attachments-list"
  >
    <Connect(Attachment)
      file={
        Object {
          "filename": "filename2.ext",
          "originalname": "A Human name 2",
        }
      }
      isSourceDocument={false}
      key="0"
      parentId="parentId"
      parentSharedId="parentSharedId"
      readOnly={false}
      storeKey="storeKey"
    />
    <Connect(Attachment)
      file={
        Object {
          "filename": "filename1.ext",
          "originalname": "Human name 1",
        }
      }
      isSourceDocument={false}
      key="1"
      parentId="parentId"
      parentSharedId="parentSharedId"
      readOnly={false}
      storeKey="storeKey"
    />
  </div>
  <Connect(NeedAuthorization)
    roles={
      Array [
        "admin",
        "editor",
      ]
    }
  >
    <div
      className="attachment-add"
    >
      <Connect(UploadAttachment)
        entity="parentId"
        storeKey="storeKey"
      />
    </div>
  </Connect(NeedAuthorization)>
</div>
`;

